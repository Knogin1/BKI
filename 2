#include <rbc.h>
#include <math.h>
//5ms=1mm
float x1,y1 = 0;

void move(float a,int n){
    setMotorSpeed(motorA, n);
    setMotorSpeed(motorD, n);
    delay(a*7.5);
}

void turn(float d) {
    if (d >= 0) {
        setMotorSpeed(motorA, -40);
        setMotorSpeed(motorD, 40);
        delay(11 * d);
    }
    else{
        setMotorSpeed(motorA, 40);
        setMotorSpeed(motorD, -40);
        delay(11 * -d);
    }

    //moveCord(

